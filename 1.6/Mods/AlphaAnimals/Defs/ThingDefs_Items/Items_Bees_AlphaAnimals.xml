<Defs>

	<!--========== Alpha Animals Branch ==========-->
	<!--=============== Aero ====================-->
	<ThingDef ParentName="RB_BeeBase" Name="RB_AeroBeeBase" Abstract="True">
		<description>Aero bees are translucent bees filled with blue-tinted hydrogen gas. They collect blue gel in their honeycombs.\n\nBee Traits:\n\nProduction time: 24 hours\nNocturnal: No\nPluviophile: No\nUnusual plants needed: None\nMinimum Temperature: 0ºC\nMaximum temperature: 30ºC</description>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<tradeability>Buyable,Sellable</tradeability>
		<comps>
			<li Class="RimBees.CompProperties_Bees">
				<species>Aero</species>
				<comb>RB_Aero_Honeycomb</comb>
				<combtimedays>1</combtimedays>
				<nocturnal>false</nocturnal>
				<pluviophile>false</pluviophile>
				<weirdplantneeded>no</weirdplantneeded>
				<tempMin>0</tempMin>
				<tempMax>30</tempMax>
			</li>
		</comps>
	</ThingDef>
	<ThingDef ParentName="RB_AeroBeeBase">
		<defName>RB_Bee_Aero_Drone</defName>
		<label>aero drone</label>
		<graphicData>
			<texPath>Things/Item/Bees/RB_Bee_Aero_Drone</texPath>
		</graphicData>
	</ThingDef>
	<ThingDef ParentName="RB_AeroBeeBase">
		<defName>RB_Bee_Aero_Queen</defName>
		<label>aero queen</label>
		<graphicData>
			<texPath>Things/Item/Bees/RB_Bee_Aero_Queen</texPath>
		</graphicData>
	</ThingDef>
	<!--=============== Black ====================-->
