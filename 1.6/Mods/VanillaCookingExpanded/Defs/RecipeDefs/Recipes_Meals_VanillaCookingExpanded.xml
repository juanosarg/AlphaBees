<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef>
		<defName>RB_CookHoneyMealGourmet</defName>
		<label>cook gourmet honey meal</label>
		<description>Cook a culinary work of art, including a delicious T-bone steak with honeyed mustard sauce, lime, ginger and honey kombucha and some honey flavoured pastries.</description>
		<jobString>Cooking gourmet honey meal.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>VEF.Cooking.IngredientValueGetter_NutritionWithExtraIngredient</ingredientValueGetterClass>
		<recipeUsers>
			<li>RB_Beekeeping_Workbench_Building</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>0.5</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>RB_Honeys</li>
					</categories>
				</filter>
				<count>0.5</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>VCE_Condiments</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
				<li>VCE_Condiments</li>
				<li>RB_Honeys</li>
			</categories>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
				<li>VCE_Condiments</li>
				<li>RB_Honeys</li>
			</categories>
			<disallowedThingDefs>
				<li>Meat_Human</li>
				<li>Meat_Megaspider</li>
				<li MayRequire="Ludeon.RimWorld.Anomaly">Meat_Twisted</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
		<workSkill>Cooking</workSkill>
		<products>
			<RB_GourmetHoney>1</RB_GourmetHoney>
		</products>
		<skillRequirements>
			<Cooking>10</Cooking>
		</skillRequirements>
	</RecipeDef>
</Defs>
