<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Nature's Pretty Sweet</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>
	


			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/BiomeDef[defName = "TKKN_Desert"]/wildPlants</xpath>
				<value>
					<RB_Plant_Hive_Desert>0.01</RB_Plant_Hive_Desert>
					<RB_Plant_Hive_Arid>0.01</RB_Plant_Hive_Arid>
				</value>
			</li>
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/BiomeDef[defName = "TKKN_Oasis"]/wildPlants</xpath>
				<value>
					<RB_Plant_Hive_Desert>0.01</RB_Plant_Hive_Desert>
					<RB_Plant_Hive_Arid>0.01</RB_Plant_Hive_Arid>
				</value>
			</li>
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/BiomeDef[defName = "TKKN_Savanna"]/wildPlants</xpath>
				<value>
					<RB_Plant_Hive_Desert>0.01</RB_Plant_Hive_Desert>
					<RB_Plant_Hive_Arid>0.01</RB_Plant_Hive_Arid>
				</value>
			</li>
			<li Class="PatchOperationAdd"> 
