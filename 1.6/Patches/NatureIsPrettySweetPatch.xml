				<value>
					<RB_Plant_Hive_Temperate>0.01</RB_Plant_Hive_Temperate>
					<RB_Plant_Hive_Mild>0.01</RB_Plant_Hive_Mild>
				</value>
			</li>
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/BiomeDef[defName = "TKKN_VolcanicFlow"]/wildPlants</xpath>
				<value>
					<RB_Plant_Hive_Arctic>0.01</RB_Plant_Hive_Arctic>
					<RB_Plant_Hive_Icy>0.01</RB_Plant_Hive_Icy>
				</value>
			</li>

			
			
			
		</operations>
